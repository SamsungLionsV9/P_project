# ML Service 환경 변수 예시
# 실제 사용 시 .env 파일로 복사하고 값을 입력하세요

# Groq AI API
GROQ_API_KEY=your_groq_api_key_here

# 한국은행 ECOS API
BOK_API_KEY=your_bok_api_key_here

# 네이버 DataLab API
NAVER_CLIENT_ID=your_naver_client_id_here
NAVER_CLIENT_SECRET=your_naver_client_secret_here

# 서버 설정
PORT=8000
HOST=0.0.0.0
RELOAD=True

# 모델 경로 (선택)
MODEL_PATH=../models
